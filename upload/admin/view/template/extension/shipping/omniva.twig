{{ header}}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <button type="submit" form="form-custom" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i></button>

        <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a>
        </div>
      <h1>{{ heading_title }}</h1>

      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
        {% endfor %}
      </ul>

    </div>
  </div>




<div class="container-fluid">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><i class="fa fa-pencil"></i> {{ heading_title }}</h3>
      </div>
      <div class="panel-body">

  <div class="container-fluid">
          <ul class="nav nav-tabs">
            <li class="active"><a href="#tab-general" data-toggle="tab">{{ tab_general }}</a></li>
        {% for geo_zone in geo_zones %}
        <li><a href="#tab-geo-zone{{ geo_zone['geo_zone_id']}}" data-toggle="tab">{{ geo_zone['name']}}</a></li>
        {% endfor %}
          </ul>

 


      <form action="{{ action}}" method="post" enctype="multipart/form-data" id="form-custom" class="form-horizontal">

       <div class="tab-content">
            <div class="tab-pane active" id="tab-general">

       
                    <div class="form-group">
                        <label class="col-sm-2 control-label" for="input-cost">{{ entry_tax_class}}</label>
                        <div class="col-sm-10">
                            <select name="shipping_omniva_tax_class_id" class="form-control">
                                <option value="0">{{ text_none}}</option>
                                {% for tax_class in tax_classes %}
                                {% if tax_class['tax_class_id'] == shipping_omniva_tax_class_id %}
                                <option value="{{ tax_class['tax_class_id']}}" selected="selected">{{ tax_class['title']}}</option>
                                {% else %}
                                <option value="{{ tax_class['tax_class_id']}}">{{ tax_class['title']}}</option>
                                {% endif %}
                                {% endfor %}
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label" for="input-cost">{{ entry_status}}</label>
                        <div class="col-sm-10">
                                <select name="shipping_omniva_status" class="form-control">
                                {% if shipping_omniva_status %}
                                <option value="1" selected="selected">{{ text_enabled}}</option>
                                <option value="0">{{ text_disabled}}</option>
                                {% else %}
                                <option value="1">{{ text_enabled}}</option>
                                <option value="0" selected="selected">{{ text_disabled}}</option>
                                {% endif %}
                                </select>
                        </div>
                    </div>
    
                    <div class="form-group">
                        <label class="col-sm-2 control-label" for="input-cost">{{ entry_disableifhtml}}</label>
                        <div class="col-sm-10">
                            <select name="shipping_omniva_disableifhtml" class="form-control">
                                {% if shipping_omniva_disableifhtml %}
                                <option value="1" selected="selected">{{ text_enabled}}</option>
                                <option value="0">{{ text_disabled}}</option>
                                {% else %}
                                <option value="1">{{ text_enabled}}</option>
                                <option value="0" selected="selected">{{ text_disabled}}</option>
                                {% endif %}
                            </select>
                        </div>
                    </div>
                  <div class="form-group">
                        <label class="col-sm-2 control-label" for="input-cost">{{ entry_maxweight}}</label>
                        <div class="col-sm-10">
                            <input type="numeric" name="shipping_omniva_maxweight" value="{{ shipping_omniva_maxweight}}" class="form-control"/>
                        </div>
                    </div>


                  <div class="form-group">
                        <label class="col-sm-2 control-label" for="input-cost">{{ entry_sort_order}}</label>
                        <div class="col-sm-10">
                            <input type="text" name="shipping_omniva_sort_order" value="{{ shipping_omniva_sort_order}}" size="1" class="form-control"/></td>
                        </div>
                </div>



            </div>
       

        {% for state in states %}

      
        <div id="tab-geo-zone{{ state['geo_zone']}}" class="tab-pane">
            <div class="form-group">
                        <label class="col-sm-2 control-label" for="input-cost">{{ entry_baseshippingprice}}</label>
                        <div class="col-sm-10">
                                <input type="text" name="shipping_omniva_{{ state['geo_zone']}}_baseshippingprice" value="{{state['baseshippingprice']}}" class="form-control"/>
                               

{{ attribute(geo_zones, fieldName) }}
                        </div>
            </div>
              <div class="form-group">
                        <label class="col-sm-2 control-label" for="input-cost">{{ entry_priceperadditional}}</label>
                        <div class="col-sm-10">
                                <input type="text" name="shipping_omniva_{{ state['geo_zone']}}_priceperadditional" value="{{state['priceperadditional']}}" class="form-control"/></td>
                        </div>
            </div>
              <div class="form-group">
                        <label class="col-sm-2 control-label" for="input-cost">{{ entry_freeshippingfrom}}</label>
                        <div class="col-sm-10">
                    <input type="text" name="shipping_omniva_{{ state['geo_zone'] }}_freeshippingfrom" value="{{state['freeshippingfrom']}}" class="form-control"/></td>
                    </div>
                </div>
                <div class="form-group">
                        <label class="col-sm-2 control-label" for="input-cost">{{ entry_status}}</label>
                        <div class="col-sm-10">
                            <select name="shipping_omniva_{{ state['geo_zone'] }}_status" class="form-control">
                                {% if state['status'] %}
                                <option value="1" selected="selected">{{ text_enabled}}</option>
                                <option value="0">{{ text_disabled}}</option>
                                {% else %}
                                <option value="1">{{ text_enabled}}</option>
                                <option value="0" selected="selected">{{ text_disabled}}</option>
                                {% endif %}
                            </select>
                        </div>
                </div>
        </div>
        {% endfor %}
      </form>
    </div>
  </div>
  </div>
  </div>
</div>
<script type="text/javascript"><!--
$('.vtabs a').tabs(); 
//--></script> 
{{ footer }}